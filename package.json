{
  "name": "np-mod-discord-bot",
  "version": "1.0.0",
  "description": "An opinionated discord built using Express, Prisma, MySql, React and JavaScript for ban tracking across channels, streamer/mod verification and communication.",
  "private": true,
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "check": "turbo run check",
    "fix": "turbo run fix",
    "start": "turbo run start --parallel",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,css,scss}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoPixel-Mod-Discord/discord-bot.git"
  },
  "author": "NoPixel Mods Discord",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NoPixel-Mod-Discord/discord-bot/issues"
  },
  "homepage": "https://github.com/NoPixel-Mod-Discord/discord-bot#readme",
  "workspaces": [
    "/*"
  ],
  "devDependencies": {
    "prettier": "^2.7.1",
    "turbo": "^1.4.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
